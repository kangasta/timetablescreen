{
  "name": "timetbl",
  "version": "0.9.4",
  "private": true,
  "main": "build/timetbl.js",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "<22.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.15.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "jest": "<22.0.0",
    "prop-types": "^15.6.0",
    "raf": "^3.4.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0"
  },
  "dependencies": {
    "chillisalmon": "https://github.com/kangasta/chillisalmon/releases/download/v0.6.1/chillisalmon-0.6.1.tgz",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "npm": "^6.5.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "scripts": {
    "build": "babel src -d build --ignore '**/__tests__/*','**/__mocks__/**',**/SetupTests.js",
    "build:watch": "babel src -w -d build --ignore '**/__tests__/*','**/__mocks__/**',**/SetupTests.js",
    "lint": "eslint src/",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/__mocks__/StyleMock.js"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testURL": "http://localhost:8080",
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    }
  }
}
